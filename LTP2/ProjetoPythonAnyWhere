print('Defesa Time 2')
print(sorteio)
lista.remove (sorteio)
sorteio = random.choice(lista)
print('Ataque Time 2')
print(sorteio)
lista.remove (sorteio)
sorteio = random.choice(lista)
print('Ataque Time 2')
print(sorteio)
valor_quadra = n11 / 10
print('Valor Da quadra para cada jogador')
print(valor_quadra)
